{
  "kind": "Dashboard",
  "metadata": {
    "name": "ceph-rgw-bucket-user",
    "project": "ceph-storage"
  },
  "spec": {
    "display": {
      "name": "ceph rgw bucket + user"
    },
    "panels": {
      "2_0": {
        "kind": "Panel",
        "spec": {
          "display": {
            "name": "Data Sent Per Bucket"
          },
          "plugin": {
            "kind": "TimeSeriesChart",
            "spec": {
              "legend": {
                "position": "bottom"
              },
              "yAxis": {
                "format": {
                  "unit": "bytes/sec"
                },
                "label": "",
                "show": true
              }
            }
          },
          "queries": [
            {
              "kind": "TimeSeriesQuery",
              "spec": {
                "plugin": {
                  "kind": "PrometheusTimeSeriesQuery",
                  "spec": {
                    "query": "topk(10,\n  sum by (bucket, user) (\n    rate(radosgw_bytes_sent[$__rate_interval])\n  )\n)",
                    "seriesNameFormat": "{{ user }} - {{ bucket }}"
                  }
                }
              }
            }
          ]
        }
      },
